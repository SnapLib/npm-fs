{
  "extends": "./tsconfig.src.json",
  "compilerOptions": {
    "declaration": false,
    "noEmitOnError": false,
    "noImplicitReturns": false,
    "noUncheckedIndexedAccess": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "strict": false
  }
}

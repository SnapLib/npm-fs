{
  "name": "snaplib-node-npm-fs",
  "version": "0.1.0",
  "description": "Node based npm file system API",
  "type": "commonjs",
  "main": "scripts/npm/init/init.js",
  "scripts": {
    "clean": "rimraf build",
    "mocha": "mocha -r ts-node/register",
    "test": "mocha -r ts-node/register src/test/ts/*.ts",
    "tsc": "tsc",
    "tsc-src": "tsc -m CommonJS -t ES2019 --sourceMap true --strict true --removeComments false -p src/main/ts/tsconfig.json --outDir build/js",
    "ts": "ts-node src/main/ts/root/RootDirectory.ts",
    "ts-node": "ts-node"
  },
  "keywords": [
    "snaplib",
    "node",
    "npm",
    "project",
    "build",
    "system"
  ],
  "author": "Snap",
  "license": "MIT",
  "devDependencies": {
    "@microsoft/tsdoc": "^0.12.21",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.0.4",
    "@types/node": "^14.14.9",
    "@types/npm": "^2.0.31",
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "npm": "^6.14.9"
  }
}

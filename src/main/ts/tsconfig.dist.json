{
  "extends": "./tsconfig.src.json",
  "compilerOptions":
  {
    "declaration": true,
    "module": "commonjs",
    "noEmitOnError": true,
    "noImplicitReturns": true,
    "noUncheckedIndexedAccess": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "preserveConstEnums": false,
    "removeComments": true,
    "sourceMap": false,
    "strict": true
  }
}

{
  "name": "snaplib-node-npm-fs",
  "version": "0.1.0",
  "description": "Node based npm file system API",
  "type": "commonjs",
  "main": "scripts/npm/init/init.js",
  "scripts": {
    "clean": "rimraf build",
    "mocha": "mocha -r ts-node/register",
    "test": "mocha -r ts-node/register src/test/ts/**.ts",
    "ts-node": "ts-node",
    "tsc": "tsc",
    "tsc-src": "tsc -m CommonJS -t ES2019 --sourceMap true --strict true --removeComments false -p src/main/ts/tsconfig.json --outDir build/js",
    "tslint": "eslint --ext .ts src/main/ts/**.ts",
    "typedoc": "typedoc --readme docs/PKG_README.md --out build/docs/ src/main/ts/**.ts"
  },
  "keywords": [
    "snaplib",
    "node",
    "npm",
    "project",
    "build",
    "system"
  ],
  "author": "Snap",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.0.4",
    "@types/node": "^14.14.9",
    "@types/npm": "^2.0.31",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "chai": "^4.2.0",
    "eslint": "^7.14.0",
    "mocha": "^8.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typedoc": "^0.19.2",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "npm": "^6.14.9"
  }
}
